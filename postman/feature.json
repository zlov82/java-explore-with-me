    {
      "name": "Получение событий подписчика где он участник",
      "event": [
        {
          "listen": "prerequest",
          "script": {
            "exec": [
              "const main = async () => {\r",
              "    const api = new API(pm);\r",
              "    const rnd = new RandomUtils();\r",
              "\r",
              "    try {\r",
              "        const firstUser = await api.addUser(rnd.getUser()); // пользователь который будет дружить\r",
              "        pm.collectionVariables.set('uid', firstUser.id);\r",
              "        \r",
              "        const user = await api.addUser(rnd.getUser()); // пользователь для создания события\r",
              "        const category = await api.addCategory(rnd.getCategory());\r",
              "        pm.collectionVariables.set('eventInitiatorId', user.id);\r",
              "        let eventBody = rnd.getEvent(category.id);\r",
              "        eventBody['requestModeration'] = false\r",
              "        let event = await api.addEvent(user.id, eventBody);\r",
              "        event = await api.publishEvent(event.id);   //перевели событие в публикацию\r",
              "        \r",
              "        const memberOfEvent = await api.addUser(rnd.getUser()); // пользователь участник события и наш будущий фолловер\r",
              "        pm.collectionVariables.set('followerId', memberOfEvent.id);\r",
              "        await api.publishParticipationRequest(event.id,memberOfEvent.id); //принял участие в событии\r",
              "        await api.addSubscriber(firstUser.id,memberOfEvent.id); //мы на него подписались\r",
              "\r",
              "\r",
              "        } catch(err) {\r",
              "        console.error(\"Ошибка при подготовке тестовых данных.\", err);\r",
              "    }\r",
              "};\r",
              "\r",
              "const interval = setInterval(() => {}, 1000);\r",
              "\r",
              "setTimeout(async () => \r",
              "    {\r",
              "        try {\r",
              "            // выполняем наш скрипт\r",
              "            await main();\r",
              "        } catch (e) {\r",
              "            console.error(e);\r",
              "        } finally {\r",
              "            clearInterval(interval);\r",
              "        }\r",
              "    },  \r",
              "    100 \r",
              ");"
            ],
            "type": "text/javascript",
            "packages": {}
          }
        },
        {
          "listen": "test",
          "script": {
            "exec": [
              "pm.test(\"Ответ должен содержать код статуса 200 и данные в формате json\", function () {\r",
              "    pm.response.to.have.status(200);\r",
              "    pm.response.to.be.withBody;\r",
              "    pm.response.to.be.json;\r",
              "});\r",
              "\r",
              "var response = pm.response.json();\r",
              "\r",
              "pm.test(\"Ответ содержит массив\", function () {\r",
              "   pm.expect(response).is.an('array');\r",
              "});\r",
              "\r",
              "pm.test(\"Ответ должен содержать поле initiator.id\", function () {\r",
              "   pm.expect(response[0].initiator).to.have.property(\"id\");\r",
              "});\r",
              "\r",
              "pm.test(\"initiator.id не равен id подписчика и не равен id клиента\", function () {\r",
              "   var privateUserId = pm.collectionVariables.set('uid');\r",
              "   var followerId = pm.collectionVariables.set('followerId')\r",
              "   pm.expect(response[0].initiator.id).to.not.equal(privateUserId);\r",
              "   pm.expect(response[0].initiator.id).to.not.equal(followerId);\r",
              "});\r",
              ""
